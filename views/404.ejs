<% if(locals.title) { %>
  <%- include('./wrappers/head', {title: title}); %>
<% } else { %>
  <%- include('./wrappers/head', {title: "Error 404"}); %>
<% } %>
<section>
  <h1>ERROR!</h1>
  <h3>
    <% if(locals.msg) { %>
      <%= msg %>
    <% } else { %>
      Page not found!
    <% } %>
  </h3>
</section>
<%- include('./wrappers/foot'); %>